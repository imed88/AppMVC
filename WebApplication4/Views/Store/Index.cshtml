@model IEnumerable<WebApplication4.Models.Tables.Specialite>

@{
    ViewBag.Title = "Index";
}

<h3>Browse Genres</h3>
<p> Select from @Model.Count() specialites:</p>
<ul>
    @foreach (var genre in Model)
    {
        @*<li>@genre.SpecialiteName</li>*@
    <li>@Html.ActionLink(genre.SpecialiteName, "Browse", new { genre = genre.SpecialiteName })</li>
    }
</ul>