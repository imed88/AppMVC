@model IEnumerable<WebApplication4.Models.Tables.Ordonnance>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/MainLayout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.IdPatients)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MatriculePatients)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NomPatient)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PrenomPatient)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.IdPatients)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MatriculePatients)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NomPatient)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PrenomPatient)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.OrdonanceID }) |
            @Html.ActionLink("Details", "Details", new { id=item.OrdonanceID }) |
