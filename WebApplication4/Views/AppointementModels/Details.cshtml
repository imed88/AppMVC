@model WebApplication4.Models.Tables.AppointementModel
<div class="container-fluid">
    <section class="content-header">
        <h1>
            Infirmerie
            <small>Rendez-vous avec les médecins conventionnés</small>
        </h1>
        <ol class="breadcrumb">
            <li><a href="#"><i class="fa fa-dashboard"></i> Accueil</a></li>
            <li>Rendez-vous</li>
            <li class="active">Informations</li>
        </ol>
    </section>
    <!-- Main content -->
    <section class="content">
        <div class="row">

            <div>
                <hr />
                <dl class="dl-horizontal">
                    <dt>
                        @*@Html.DisplayNameFor(model => model.ApplicationUser.UserName)*@
                        @Html.Label("Administrateur", htmlAttributes: new { @class = "control-label col-md-15" })
                    </dt>
                    <dd>
                        @Html.DisplayFor(model => model.ApplicationUser.UserName)
                    </dd>
                    <dt>
                        @*@Html.DisplayNameFor(model => model.Doctor.idDoctors)*@
                        @Html.Label("Médecin", htmlAttributes: new { @class = "control-label col-md-15" })
                    </dt>
                    <dd>
                        @Html.DisplayFor(model => model.Doctor.nameDoctors)
                    </dd>
                    <dt>
                        @*@Html.DisplayNameFor(model => model.Patient.NomPatient)*@
                        @Html.Label("Nom du patient", htmlAttributes: new { @class = "control-label col-md-15" })
                    </dt>
                    <dd>
                        @Html.DisplayFor(model => model.Patient.NomPatient)
                    </dd>
                    <dt>
                        @*@Html.DisplayNameFor(model => model.Patient.PrenomPatient)*@
                        @Html.Label("Prénom du patient", htmlAttributes: new { @class = "control-label col-md-15" })
                    </dt>
                    <dd>
                        @Html.DisplayFor(model => model.Patient.PrenomPatient)
                    </dd>
                   
                   

                </dl>
            </div>
            <p>
                @*@Html.ActionLink("Modifier", "Edit", new { id = Model.AppointmentID })*@
                <a href="@Url.Action("Edit", "AppointementModels",new { id = Model.AppointmentID })" class="btn btn-warning">
                    <i class="glyphicon glyphicon-pencil"></i>
                    <span>
                        <strong></strong>
                    </span>
                </a> |
                @*@Html.ActionLink("Retour", "Index")*@
                |
                <a href="@Url.Action("Index", "AppointementModels")" class="btn btn-default">
                    <i class="glyphicon glyphicon glyphicon-chevron-left"></i>
                    <span>
                        <strong>Retour</strong>
                    </span>
                </a>
            </p>
        </div>
    </section>
</div>